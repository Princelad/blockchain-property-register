# Hyperledger Fabric Network Environment Variables

# Network Configuration
COMPOSE_PROJECT_NAME=property-register-network
NETWORK_NAME=fabric-network

# Fabric Version
FABRIC_VERSION=2.5.4
FABRIC_CA_VERSION=1.5.7

# Channel Configuration
CHANNEL_NAME=property-channel

# Chaincode Configuration
CHAINCODE_NAME=property-cc
CHAINCODE_VERSION=1.0
CHAINCODE_PATH=github.com/hyperledger/fabric-samples/chaincode/abstore/go
CHAINCODE_LANGUAGE=golang

# Orderer Configuration
ORDERER_HOST=orderer.example.com
ORDERER_PORT=7050
ORDERER_DOMAIN=example.com

# Organization 1 - Government Authority
ORG1_NAME=Gov
ORG1_DOMAIN=gov.example.com
ORG1_MSP_ID=Org1MSP
ORG1_CA_PORT=7054
ORG1_PEER0_PORT=7051
ORG1_PEER1_PORT=8051

# Organization 2 - Notary Services
ORG2_NAME=Notary
ORG2_DOMAIN=notary.example.com
ORG2_MSP_ID=Org2MSP
ORG2_CA_PORT=8054
ORG2_PEER0_PORT=9051
ORG2_PEER1_PORT=10051

# Organization 3 - Property Registry
ORG3_NAME=Registry
ORG3_DOMAIN=registry.example.com
ORG3_MSP_ID=Org3MSP
ORG3_CA_PORT=9054
ORG3_PEER0_PORT=11051
ORG3_PEER1_PORT=12051

# Orderer CA
ORDERER_CA_PORT=10054

# Endorsement Policy
ENDORSEMENT_POLICY="AND('Org1MSP.peer','Org2MSP.peer','Org3MSP.peer')"

# TLS Configuration
TLS_ENABLED=false

# Database Configuration
STATE_DATABASE=LevelDB

# Logging
LOG_LEVEL=info
